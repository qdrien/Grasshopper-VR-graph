                </item>
                thousand 
                <tag> out._value = out._value * 1000; </tag>
                <item repeat="0-1">
                    <item repeat="0-1"> and </item>
                    <ruleref uri="#Cardinal_1_to_999"/>
                    <tag> out._value = out._value + rules.latest()._value; </tag>
